{
  "dependencies": {
    "@octokit/rest": "^15.12.0",
    "@types/debug": "^0.0.31",
    "@types/node": "^10.12.0",
    "@types/promise-retry": "^1.1.2",
    "debug": "^4.0.1",
    "github-rebase": "^0.4.0",
    "probot": "^7.2.0",
    "promise-retry": "^1.1.1",
    "shared-github-internals": "^0.2.0",
    "typescript": "^3.1.3"
  },
  "description": "GitHub App to automatically rebase and merge pull requests",
  "devDependencies": {
    "@types/jest": "^23.3.5",
    "@types/uuid": "^3.4.4",
    "jest": "^23.6.0",
    "jest-junit": "^5.1.0",
    "nodemon": "^1.18.4",
    "prettier": "^1.14.3",
    "smee-client": "^1.0.2",
    "ts-jest": "^23.10.4",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "uuid": "^3.3.2"
  },
  "engines": {
    "node": ">= 8.12.0"
  },
  "main": "app",
  "name": "autorebase",
  "nodemonConfig": {
    "exec": "yarn build && yarn start",
    "watch": [
      ".env",
      "src"
    ]
  },
  "private": true,
  "scripts": {
    "build": "tsc --build tsconfig.build.json",
    "dev": "nodemon",
    "prettier": "prettier --ignore-path .gitignore \"**/*.{js,json,md,ts,yml}\"",
    "start": "probot run .",
    "test": "jest",
    "tslint": "tslint --format stylish --project ."
  }
}
